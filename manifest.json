{
    "name": "VocabRiver",
    "version": "0.0",
    "description": "Build your vocabulary naturally from your browsing history",
    "permissions": [
        "storage",
        "history",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "icons": {
        "16": "icons/icon16-white.png",
        "32": "icons/icon32-white.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
    "action": {},
    "options_page": "settings.html",
    "manifest_version": 3
}