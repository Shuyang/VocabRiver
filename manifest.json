{
    "name": "VocabRiver",
    "version": "0.0",
    "description": "Build your vocabulary naturally from your browsing history",
    "permissions": [
        "storage",
        "history"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "chrome_url_overrides" : {
        "newtab": "flashcards.html"
    },
    "action": {},
    "options_page": "settings.html",
    "manifest_version": 3
}